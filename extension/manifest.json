{
    "manifest_version": 2,
    "name": "CoronavirusBlocker-Corona-block",
    "version": "2.0.1",
    "description": "Enjoy corona-free browsing on all sites.",
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ]
}